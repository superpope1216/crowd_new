package com.wisedu.crowd.entity.datacode;

/**
 * 数据库修改纪录
 * @author de
 *
 */
public enum BeanState {

	/**
	 * 新增
	 */
	ADD("1","新增"),
	/**
	 * 修改
	 */
	MODIFY("2","修改"),
	/**
	 * 删除
	 */
	DELETE("3","删除");
	private String code;
	
	private String name;
	BeanState(String code,String name){
		this.code=code;
		this.name=name;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
