<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wisedu.crowd.dao.xtgl.extend.XtcsbInfoExtendMapper" >
 <resultMap id="BaseResultMap" type="com.wisedu.crowd.entity.xtgl.XtcsbInfo" >
    <id column="WID" property="wid" jdbcType="VARCHAR" />
    <result column="CSMC" property="csmc" jdbcType="VARCHAR" />
    <result column="CSZ" property="csz" jdbcType="VARCHAR" />
    <result column="CSSM" property="cssm" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    WID, CSMC, CSZ, CSSM
  </sql>
  
  <resultMap id="BaseDisplayResultMap" type="com.wisedu.crowd.entity.xtgl.extend.XtcsbInfoExtend"  
  extends="BaseResultMap"></resultMap>
  <sql id="sqlWhere">
  		<if test='wid!=null and wid!=""'>
  			AND t_crowd_xtgl_xtcsb.WID=#{wid,jdbcType=VARCHAR}
  		</if>
  		<if test='csmc!=null and csmc!=""'>
  			AND t_crowd_xtgl_xtcsb.CSMC=#{csmc,jdbcType=VARCHAR}
  		</if>
  </sql>
  <select id="selectByCondition" resultMap="BaseDisplayResultMap" parameterType="com.wisedu.crowd.entity.xtgl.extend.XtcsbInfoExtend">
  		SELECT 
  		<include refid="Base_Column_List"/>
  		FROM t_crowd_xtgl_xtcsb t_crowd_xtgl_xtcsb 
  		<where>
  			<include refid="sqlWhere"/>
  		</where>
  		order by wid
  </select>
  
</mapper>