						kfzzhmxInfo.setLyxmid(rwjbxxInfo.getWid());
						kfzzhmxInfo.setSzlbid(SzlbEnum.XMSR.getCode());
						kfzzhmxInfo.setSzsj(DateUtil.getCurrentDate());
						kfzzhmxInfo.setWid(StringUtil.getUuId());
