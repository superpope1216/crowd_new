.load-box {
	background: #fff url(../plugins/vendor/slick/ajax-loader.gif) center
		no-repeat;
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 99999999999;
	transition: .3s;
	-moz-transition: .3s;
	-ms-transition: .3s;
	-o-transition: .3s;
	-webkit-transition: .3s;
	visibility: visible;
	opacity: 1;
}

.load-box.active {
	opacity: 0;
	visibility: hidden;
}

