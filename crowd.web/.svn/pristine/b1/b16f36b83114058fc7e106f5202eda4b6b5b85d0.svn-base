.search-box {
	float: right;
	/**margin: 25px 0;*/
	margin: 10px 0;
	position: relative;
	z-index: 8;
}

.search-box p {
	position: absolute;
	top: 50%;
	right: 55px;
	z-index: 2;
	transition: .3s;
	-moz-transition: .3s;
	-ms-transition: .3s;
	-o-transition: .3s;
	-webkit-transition: .3s;
}

.search-box.active p {
	visibility: hidden;
	opacity: 0;
}

.search-box p a {
	float: left;
	background: #eee;
	color: #757575;
	margin-right: 5px;
	transition: .2s;
	-moz-transition: .2s;
	-ms-transition: .2s;
	-o-transition: .2s;
	-webkit-transition: .2s;
	padding: 0 5px;
	line-height: 24px;
	margin-top: -12px;
	font-size: 12px;
}

.search-box p a:hover {
	background: #62a8ea;
	color: #fff;
}

.search-box form {
	display: block;
	background: #fff;
	transition: .2s;
	-moz-transition: .2s;
	-ms-transition: .2s;
	-o-transition: .2s;
	-webkit-transition: .2s;
}

.search-box.active form {
	box-shadow: 0 2px 3px rgba(0, 0, 0, .03);
}

.search-box form:after {
	display: block;
	content: '';
	clear: both;
}

.search-box form input[type=text] {
	width: 240px;
	padding: 14px;
	height: 50px;
	line-height: 20px;
	transition: .2s;
	-moz-transition: .2s;
	-ms-transition: .2s;
	-o-transition: .2s;
	-webkit-transition: .2s;
	background: none;
	border: 1px solid #e0e0e0;
	border-right: none;
	outline: none;
	float: left;
}

.search-box.active form input[type=text] {
	border-color: #62a8ea;
}

.search-box form button {
	width: 50px;
	height: 50px;
	line-height: 48px;
	background: none;
	font-size: 18px;
	color: #616161;
	transition: .2s;
	-moz-transition: .2s;
	-ms-transition: .2s;
	-o-transition: .2s;
	-webkit-transition: .2s;
	border: none;
	border: 1px solid #e0e0e0;
	float: left;
}

.search-box.active form button {
	border-color: #62a8ea;
}

.search-box form button:hover {
	background: #62a8ea;
	border-color: #62a8ea;
	color: #fff;
}

