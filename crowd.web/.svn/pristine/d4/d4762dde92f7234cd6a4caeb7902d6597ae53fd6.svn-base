#aside{
    width: 140px;
    height: 120px;
    position: fixed;
    right: 100px;
    top:460px;
    z-index: 3;
    margin-right:2%;
}
#aside li{
    height: 24px;
    margin-right:-40px;
    line-height: 24px;
    cursor: pointer;
    color: #333;
    padding-left: 20px;
    box-sizing: border-box;
   
}

#aside li.active{
    color: #000;
    background: #e7e6eb;
    border-left: 4px solid #1e293f;
}